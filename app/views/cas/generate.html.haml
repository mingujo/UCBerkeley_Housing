%center
    %main.header_block Generate New Spreadsheet
= form_tag cas_path do #change

    = label :ca, :month, 'Month', :class => "edit_label"
    = text_field :ca, 'month'

    = label :ca, :year, 'Year', :class => "edit_label"
    = text_field :ca, 'year'

    %br
    = submit_tag 'Generate', :class => "savebutton"
    
=button_to 'CANCEL', cas_path, :method => :get, :class => "deletebutton"